{
  "name": "@c4605/react-components",
  "version": "0.2.1",
  "license": "MIT",
  "dependencies": {
    "@c4605/ts-types": "^0.1.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-side-effect": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@c4605/toolconfs": "^0.1.0",
    "@sambego/storybook-state": "^1.3.2",
    "@storybook/addon-actions": "^4.1.1",
    "@storybook/addon-info": "^4.1.1",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-options": "^4.1.3",
    "@storybook/addon-storyshots": "^4.1.4",
    "@storybook/addon-storysource": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.0.11",
    "@types/react-side-effect": "^1.1.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^4.0.0",
    "@types/storybook__addon-options": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "@types/webpack": "^4.4.21",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-auto-import": "^1.0.0",
    "babel-plugin-macros": "^2.4.3",
    "css-loader": "^2.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "git-hook-pure": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-test-renderer": "^16.7.0",
    "require-context.macro": "^1.0.4",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.3.0-rc",
    "webpack": "^4.28.3"
  },
  "scripts": {
    "prettier-js": "prettier --parser flow --write",
    "prettier-ts": "prettier --parser typescript --write",
    "prettier-css": "prettier --parser css --write",
    "storybook": "TS_NODE_FILES=true TS_NODE_PROJECT=./configs/tsconfig.node.json start-storybook -p 9001 -c ./configs/storybook --ci",
    "prepublish": "npm run compile",
    "compile": "rm -rf lib && tsc -p ./configs/tsconfig.compile.json",
    "test": "NODE_ENV=test yarn jest --no-cache src"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run prettier-js",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint -p .",
      "npm run prettier-ts",
      "git add"
    ],
    "*.{css,scss,less}": [
      "npm run prettier-css",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./configs/jestSetup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "html"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ]
}
