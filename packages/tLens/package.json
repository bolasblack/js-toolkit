{
  "name": "tLens",
  "version": "0.1.0",
  "description": "Type safe lens toolkit for typescript",
  "main": "lib/index.js",
  "author": "c4605 <bolasblack@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ts-node": "TS_NODE_FILES=true TS_NODE_PROJECT=./configs/tsconfig.node.json ts-node",
    "gen-code": "yarn ts-node ./scripts/render-template.ts",
    "test:dts": "dtslint --onlyTestTsNext dtslint",
    "test:watch:dts": "yarn test:dts && watch -p src/**/* -p dtslint/**/* -c 'yarn test:dts'",
    "test:watch": "yarn test:watch:dts"
  },
  "lint-staged": {
    "{src,configs,dtslint,scripts}/**/*": [
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@c4605/ts-types": "^0.2.0",
    "@types/ramda": "types/npm-ramda#dist",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.1",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.120",
    "dtslint": "^0.5.0",
    "ejs": "^2.6.1",
    "glob": "^7.1.3",
    "lint-staged": "^8.1.3",
    "lodash": "^4.17.11",
    "node-notifier-cli": "^1.1.2",
    "prettier": "^1.16.4",
    "rxjs": "^6.3.3",
    "rxjs-stream": "^3.0.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3",
    "watch-cli": "^0.2.3"
  }
}
