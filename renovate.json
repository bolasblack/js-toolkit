{
  "extends": ["config:base", ":semanticCommitTypeAll(chore)"],
  "ignoreDeps": ["docz", "docz-theme-default"],
  "baseBranches": ["develop"],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "rangeStrategy": "bump"
    },
    {
      "paths": ["packages/*"],
      "depTypeList": ["dependencies", "peerDependencies"],
      "rangeStrategy": "widen"
    }
  ]
}
