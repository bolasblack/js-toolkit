{
  "extends": ["config:base", ":semanticCommitTypeAll(chore)"],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "excludePackagePatterns": ["^docz"],
      "rangeStrategy": "bump"
    },
    {
      "paths": ["packages/*"],
      "depTypeList": ["dependencies", "peerDependencies"],
      "rangeStrategy": "widen"
    }
  ]
}
